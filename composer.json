{
    "name": "jamarcer/openapi-schema-parsers",
    "description": "OpenAPI / AsyncApi parser utilities",
    "version": "1.0.0",
    "license": "MIT",
    "type": "library",
    "authors": [
    {
        "name": "Jesús Martínez",
        "email": "jamarcer@gmail.com"
    }
    ],
    "autoload": {
        "psr-4": {
            "Jamarcer\\OpenApiMessagingContext\\\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Jamarcer\\Tests\\OpenApiMessagingContext\\": "tests/"
        }
    },
    "require": {
        "ext-json": "*",
        "php": "^8.0",
        "symfony/yaml": "^5.0",
        "symfony/messenger": "^5.0",
        "justinrainbow/json-schema": "^5.2",
        "behat/behat": "^3.5",
        "jamarcer/ddd-framework": "dev-main",
        "beberlei/assert": "^3.2",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/mink-browserkit-driver": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.0",
        "dg/bypass-finals": "^1.1"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jamarcer/ddd-framework"
        }
    ]
}
